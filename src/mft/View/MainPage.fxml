<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mft.Controller.MainPageController">
  <children>
    <AnchorPane fx:id="MainPage" maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
            <MenuBar layoutY="-2.0" prefHeight="27.0" prefWidth="646.0">
              <menus>
                <Menu mnemonicParsing="false" text="Resources">
                  <items>
                        <MenuItem fx:id="ManageResourcesMenu" mnemonicParsing="false" onAction="#handleManageResourcesAction" text="Manage Resources" />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Members">
                  <items>
                        <MenuItem mnemonicParsing="false" onAction="#handleManageMemberAction" text="Manage Member" />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Requests">
                  <items>
                    <MenuItem mnemonicParsing="false" text="About" />
                  </items>
                </Menu>
              </menus>
            </MenuBar>
      </children>
    </AnchorPane>
  </children>
</VBox>
